{
    "name": "misantron/php-sendsay",
    "description": "Object-oriented interface to Sendsay API.",
    "type": "library",
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Sendsay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sendsay\\Tests\\": "tests/"
        }
    }
}
